func join(list, sep)
    var res = ""
    for i = 0, len(list)
        var res = res + list/i + (if i == len(list) - 1 then "" else sep)
    end
    return res
end
func remove(list, x)
    var new_list = []
    for i = 0, len(list) do if list#i != x then var new_list = new_list + list#i
    return new_list
end